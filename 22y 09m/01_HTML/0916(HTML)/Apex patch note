          }
          :host([color="dark"]) .eapl-tile__link:focus {
            outline: solid var(--ea-dark-color) 4px;
          }
          :host([color="dark"]) .eapl-tile__link:focus::before {
            box-shadow: inset 0 0 0 4px white;
          } /* overlay type focus */
          :host([type="overlay"]) .eapl-tile__link:focus {
            outline: solid var(--ea-dark-color) 4px;
          }
          :host([type="overlay"]) .eapl-tile__link:focus::before {
