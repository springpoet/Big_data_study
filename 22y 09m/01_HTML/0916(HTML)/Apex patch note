          }
          .eapl-tile__link[modal-id] {
            cursor: pointer;
          } /* horizontal variant */
          :host([type="horizontal"]) .eapl-tile__link {
            display: flex;
            flex-direction: column;
          }
          @media only screen and (min-width: 768px) {
            /* horizontal variant */
            :host([type="horizontal"]) .eapl-tile__link {
              flex-direction: row;
            }
          } /* focus */
          .eapl-tile__link:focus {
            outline: solid white 4px;
            outline-offset: 0;
          }
          .eapl-tile__link:focus::before {
            bottom: 0;
            box-shadow: inset 0 0 0 4px var(--ea-dark-color);
            content: "";
            left: 0;
            position: absolute;
            right: 0;
            top: 0;
