            transform-origin: top left;
            transition: transform 0.25s cubic-bezier(0.5, 0.25, 0.015, 1);
            width: 100%;
          }
          :host([rtl]) .eapl-input__label {
            transform-origin: top right;
          }
          @media only screen and (min-width: 1024px) {
            .eapl-input__label {
              top: 22px;
            }
          }
          @media only screen and (min-width: 1456px) {
            .eapl-input__label {
              top: 24px;
            }
          } /* light variant */
          :host([text-color="light"]) .eapl-input__label {
            color: #585858;
          } /* current and :not(_empty) */
          :host([current]) .eapl-input__label,
          :host(:not([_empty])) .eapl-input__label {
            transform: translateY(-13px) scale(0.75);
          }
          @media only screen and (min-width: 1456px) {
            :host([current]) .eapl-input__label,
            :host(:not([_empty])) .eapl-input__label {
              transform: translateY(-15px) scale(0.75);
            }
          } /** * Iron Input */
